string gameOfThrones(string s) {
    unordered_map<char, int> freq;
    
    for (char c : s) {
        freq[c]++;
    }
    
    int oddCount = 0;
    for (auto pair : freq) {
        if (pair.second % 2 != 0) {
            oddCount++;
        }
    }
    
    if (oddCount > 1) {
        return "NO";
    }
    
    return "YES";
}
