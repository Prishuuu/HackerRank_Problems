void countSort(vector<vector<string>> arr) {
    int n = arr.size();
    vector<vector<string>> buckets(100);

    for (int i = 0; i < n / 2; i++) {
        arr[i][1] = "-";
    }

    for (int i = 0; i < n; i++) {
        int index = stoi(arr[i][0]);
        buckets[index].push_back(arr[i][1]);
    }

    for (int i = 0; i < 100; i++) {
        for (const string& str : buckets[i]) {
            cout << str << " ";
        }
    }
    cout << endl;
}
