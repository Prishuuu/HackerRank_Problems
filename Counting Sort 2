vector<int> countingSort(vector<int> arr) {
    vector<int> freq(100, 0);
    vector<int> sortedArr;

    for (int i = 0; i < arr.size(); i++) {
        freq[arr[i]]++;
    }

    for (int i = 0; i < freq.size(); i++) {
        for (int j = 0; j < freq[i]; j++) {
            sortedArr.push_back(i);
        }
    }

    return sortedArr;
}
